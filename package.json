{
  "name": "langstack",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p prisma:generate vite server",
    "vite": "vite",
    "server": "npx nodemon --watch './server/**/*.ts' --exec npx ts-node --project ./tsconfig.server.json --files ./server/index.ts",
    "build": "run-p build:react build:server",
    "build:react": "tsc && vite build",
    "build:server": "tsc --project ./tsconfig.server.json && vite build",
    "prisma:generate": "npx prisma generate",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@prisma/client": "^3.15.1",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "moment": "^2.29.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-loader-spinner": "^5.1.5",
    "react-router-dom": "^6.3.0",
    "react-swipeable-views": "^0.14.0",
    "styled-components": "^5.3.5",
    "uuidjs": "^4.2.11"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-loader-spinner": "^4.0.0",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/styled-components": "^5.1.25",
    "@types/uuidjs": "^3.6.0",
    "@vitejs/plugin-react": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.15.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
